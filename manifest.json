{
  "manifest_version": 2
, "name": "Github pull request review"
, "version": "0.1.3"
, "homepage_url": "https://github.com/micho/github_issues_review_chrome_extension"
, "description": "Github Issues peer review: track reviews and votes for pull requests"
, "permissions": [
    "https://github.com/",
    "cookies",
    "storage",
    "unlimitedStorage",
    "background"]
, "content_scripts": [
    {
      "matches": ["https://github.com/*", "*://*/*/issues*", "*://*/*/pull/*"]
    , "js": ["lib/jquery-1.8.2.js","lib/underscore.js","lib/underscore_mixins.js","pull.js","issues.js"]
    }
  ]
}
